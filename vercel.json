{
  "version": 2,
  "builds": [
    {
      "src": "apps/api/public/index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "apps/api/public/index.php"
    },
    {
      "src": "/(.*)",
      "dest": "apps/api/public/index.php"
    }
  ],
  "functions": {
    "apps/api/public/index.php": {
      "runtime": "@vercel/php@2.0.0"
    }
  },
  "env": {
    "APP_ENV": "prod",
    "APP_DEBUG": "false"
  }
}
