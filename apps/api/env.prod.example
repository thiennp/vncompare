# Production Environment Variables for Render
APP_ENV=prod
APP_DEBUG=false
APP_SECRET=your-32-character-secret-key-here

# Database (will be set by Render automatically)
DATABASE_URL=postgresql://user:password@host:5432/database

# JWT Configuration
JWT_SECRET_KEY=%kernel.project_dir%/config/jwt/private.pem
JWT_PUBLIC_KEY=%kernel.project_dir%/config/jwt/public.pem
JWT_PASSPHRASE=your-jwt-passphrase-here

# CORS Configuration
CORS_ALLOW_ORIGIN=https://vncompare.com,https://www.vncompare.com,http://localhost:3000

# Mailer (optional - for email features)
MAILER_DSN=smtp://localhost:1025

# Cache (Redis - optional)
REDIS_URL=redis://localhost:6379
